<?xml version="1.0" encoding="UTF-8" ?>
<style-scheme id="norka-sepia" _name="Norka Sepia" version="1.0">
  <author>Andrey Maksimov</author>
  <description>Stay focused.</description>

  <style name="text" foreground="#494F52" background="#FDF6E3" />
  <style name="cursor" foreground="#00897b" />
  <style name="current-line" background="#c1a073" />
  <style name="search-match" foreground="#494F52" background="#ffbb4a" />
  <style name="selection" foreground="#f7f7f7" background="#5299EA" />

  <!-- Bold -->
  <style name="def:statement" bold="true" />

  <!-- Italic -->
  <style name="def:doc-comment-element" foreground="#b19063" italic="true" />

  <!-- Links -->
  <style name="def:comment" foreground="#00897b" />
  <style name="def:underlined" foreground="#00897b" />

  <!-- Code -->
  <style name="def:identifier" foreground="#c1a075" />

  <!-- Line-break -->
  <style name="def:note" foreground="#3b3228" />

  <!-- Label -->
  <style name="def:preprocessor" foreground="#3b3228" />

  <!-- Image marking in Links -->
  <style name="def:shebang" foreground="#00897b" />
  <style name="def:constant" foreground="#00897b" />

  <!-- Backslash Escaping -->
  <style name="def:special-char" foreground="#c1a073" />

  <!-- Headers -->
  <style name="def:type" foreground="#3b3228" bold="true" />

  <!-- Markdown -->
  <style name="markdown:abbreviation" foreground="#575757" />
  <style name="markdown:blockquote-marker" foreground="#be6e7c" italic="true" />
  <style name="markdown:definition-list-marker" foreground="#FFFFFF" background="#575757" bold="true" />
  <style name="markdown:emphasis" italic="true" />
  <style name="markdown:header" foreground="#409aea" bold="true" />
  <style name="markdown:horizontal-rule" foreground="#409aea" bold="true" />
  <style name="markdown:label" bold="true" />
  <style name="markdown:list-marker" foreground="#C43E2C" bold="true" />
  <style name="markdown:strong-emphasis" bold="true" />
  <style name="markdown:table-separator" bold="true" />
  <style name="markdown:url" foreground="#678A00" underline="true" />
</style-scheme>
